{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "airbnb",
    "rules": {
      "react/prop-types": 2,
      "comma-dangle": [0, "never"],
      "func-names": 0,
      "no-console": 0,
      "eol-last": 0,
      "max-len": [1, 120, 2],
      "no-return-assign": 0,
      "arrow-body-style": [0],
      "react/prefer-stateless-function": 0,
      "no-underscore-dangle": 0,
      "import/extensions": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/no-static-element-interactions": 1,
      "react/no-array-index-key": 0,
      "react/no-unused-prop-types": 0,
      "react/require-default-props": 1,
      "react/sort-comp": 0,
      "class-methods-use-this": 0,
      "consistent-return": 1,
      "prefer-spread": 0,
      "padded-blocks": 1
    }

}
